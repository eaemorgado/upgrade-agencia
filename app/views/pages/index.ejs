                <div class="dashboard">
                    <img src="img/icon-dashboards.svg" alt="" class="icone-servicos">
                    <h4 class="titulo-jobs">DASHBOARDS E RELATÓRIOS</h4>
                </div>
            </div>

            <div class="linha-2">
                <div class="sites">
                    <img src="img/icon-sites.svg" alt="" class="icone-servicos">
                    <h4 class="titulo-jobs">SITES E SISTEMAS WEB</h4>
                </div>

                <div class="social-media">
                    <img src="img/icon-social.svg" alt="" class="icone-servicos">
                    <h4 class="titulo-jobs">SOCIAL MEDIA</h4>
                </div>

                <div class="trafego">
                    <img src="img/icon-trafego.svg" alt="" class="icone-servicos">
                    <h4 class="titulo-jobs">TRÁFEGO PAGO</h4>
                </div>
            </div>
            
